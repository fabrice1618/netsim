{
  "name": "netsim-v2",
  "version": "2.0.0",
  "description": "Modern network simulator built with Vue 3 - Educational tool for network topology visualization and simulation",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.4.21",
    "pinia": "^2.1.7",
    "vue-router": "^4.3.0",
    "konva": "^9.2.0",
    "vue-konva": "^3.0.2",
    "d3": "^7.9.0",
    "@headlessui/vue": "^1.7.19",
    "@heroicons/vue": "^2.1.1",
    "lodash-es": "^4.17.21",
    "uuid": "^9.0.1",
    "file-saver": "^2.0.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/test-utils": "^2.4.5",
    "vite": "^5.2.8",
    "vitest": "^1.4.0",
    "tailwindcss": "^3.4.3",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.25.0",
    "prettier": "^3.2.5",
    "playwright": "^1.43.1",
    "@types/d3": "^7.4.3",
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/uuid": "^9.0.8",
    "typescript": "^5.4.5",
    "vue-tsc": "^2.0.13"
  },
  "keywords": [
    "network",
    "simulator",
    "education",
    "vue3",
    "networking",
    "topology",
    "dhcp",
    "dns",
    "http",
    "router",
    "switch"
  ],
  "author": "NetSim Team",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/netsim-v2.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/netsim-v2/issues"
  },
  "homepage": "https://github.com/your-org/netsim-v2#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
